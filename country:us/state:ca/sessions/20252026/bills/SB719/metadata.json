  "identifier": "SB719",
  "title": "Department of Technology: inventory: high-risk automated decision systems.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "DepartmentofTechnology",
    "high-riskautomateddecisionsystems",
